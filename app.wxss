/**app.wxss**/
@import 'weui.wxss';
/* page */
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page-head{
  padding: 60rpx 50rpx 80rpx;
  text-align: center;
}
.page-head-title{
  display: inline-block;
  padding: 0 40rpx 20rpx 40rpx;
  font-size: 48rpx;
  color: #8EE5EE;
}
.page-head-line{
  margin: 0 auto;
  width: 200rpx;
  height: 2rpx;
  background-color: #8EE5EE;
}
.page-head-desc{
  padding-top: 20rpx;
  color: #9B9B9B;
  font-size: 32rpx;
}
.text{
  font-size: 30rpx;
  text-align: center;
  text-align-last: center
}